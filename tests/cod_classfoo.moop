class Foo {
    int x;
    
    Foo Foo(int x) { 
        this.x = x;
    }

    int getX() {
        return this.x;
    }

    void setX(int x) {
        this.x = x;
    }

}

class Main { 
    int main () { 
        Foo test;
        test = new Foo(45);

        if (test.getX() == 45){
            print ("here");
        }

        unless (test.getX() == 30) {
            test.setX(10);
            printi(test.getX());
        }

        unless (test.getX() == 10) {
            test.setX(15);
            printi(test.getX());
        }

        printi(test.getX());

        return 0;
    }
}

